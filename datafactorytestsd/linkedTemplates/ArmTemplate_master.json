{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorytestsd"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CosmosDbMongoDb_MongoTest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDb_MongoTest'"
		},
		"CosmosDbMongoDb_MongoTest_account": {
			"type": "secureString",
			"metadata": "Secure string for 'account' of 'CosmosDbMongoDb_MongoTest'",
			"defaultValue": "mongodb-test-sarah"
		},
		"MongoDBSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDBSource'"
		},
		"MongoDBSource_account": {
			"type": "secureString",
			"metadata": "Secure string for 'account' of 'MongoDBSource'",
			"defaultValue": "mongodb-test-sarah"
		},
		"CosmosDbMongoDb_MongoTest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SampleDB"
		},
		"MongoDBSource_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SampleDB"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorytestsd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CosmosDbMongoDb_MongoTest_connectionString": {
						"value": "[parameters('CosmosDbMongoDb_MongoTest_connectionString')]"
					},
					"CosmosDbMongoDb_MongoTest_account": {
						"value": "[parameters('CosmosDbMongoDb_MongoTest_account')]"
					},
					"MongoDBSource_connectionString": {
						"value": "[parameters('MongoDBSource_connectionString')]"
					},
					"MongoDBSource_account": {
						"value": "[parameters('MongoDBSource_account')]"
					},
					"CosmosDbMongoDb_MongoTest_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDb_MongoTest_properties_typeProperties_database')]"
					},
					"MongoDBSource_properties_typeProperties_database": {
						"value": "[parameters('MongoDBSource_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}